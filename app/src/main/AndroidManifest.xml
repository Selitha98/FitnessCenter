<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnesscenter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessCenter">
        <activity
            android:name=".Screen.UserBookingStatusActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.UserBookingActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.UpdatePackageActivity"
            android:exported="false" />
        <activity
            android:name=".Screen.UserListActivity"
            android:exported="false" />
        <activity
            android:name=".Screen.AddPackgeActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.AdminHomeActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.AdminLoginActivity"
            android:exported="true" />
        <activity
            android:name=".Screen.BookingActivity"
            android:exported="false" />
        <activity
            android:name=".Screen.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".Screen.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Screen.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>